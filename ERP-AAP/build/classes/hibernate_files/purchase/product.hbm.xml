<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.purchase">
  <class name="Product" table="product">
  <id name="product_id" column="product_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="product_category" column="product_category" type="integer"></property>
  <property name="product" column="product" type="string"></property>
  <property name="description" column="description" type="string"></property>
  <property name="product_type" column="product_type" type="integer"></property>
  <property name="created_by" column="created_by" type="integer"></property>
  <property name="updated_by" column="updated_by" type="integer"></property>
  <property name="active_flag" column="active_flag" type="string"></property>
  </class>
</hibernate-mapping>