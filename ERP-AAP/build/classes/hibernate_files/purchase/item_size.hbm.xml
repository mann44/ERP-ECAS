<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.purchase">
  <class name="Item_Size" table="add_item_size">
  <id name="size_id" column="size_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="item" column="item" type="integer"></property>
  <property name="size" column="size" type="string"></property>
  <property name="created_by" column="created_by" type="integer"></property>
  </class>
</hibernate-mapping>