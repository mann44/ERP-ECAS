<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.user_mgmt">
  <class name="Privilege" table="web_acm_privilege_mst">
  <id name="privilege_id" column="privilege_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="privilege_name" column="privilege_name" type="string"></property>
  <property name="privilege_desc" column="privilege_desc" type="string"></property>
  <property name="created_by" column="created_by" type="integer"></property>
  </class>
</hibernate-mapping>