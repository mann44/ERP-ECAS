<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.sales">
  <class name="Scheduled_Call" table="scheduled_calls">
  <id name="schedule_id" column="schedule_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="summary" column="summary" type="string"></property>
  <property name="date" column="date" type="string"></property>
  <property name="contact" column="contact" type="integer"></property>
  <property name="responsible" column="responsible" type="integer"></property>
  <property name="category" column="category" type="integer"></property>
  <property name="priority" column="priority" type="integer"></property>
  <property name="phone_no" column="phone_no" type="string"></property>
  <property name="mobile_no1" column="mobile_no1" type="string"></property>
  <property name="mobile_no2" column="mobile_no2" type="string"></property>
  <property name="description" column="description" type="string"></property>
  <property name="created_by" column="created_by" type="integer"></property>
  <property name="updated_by" column="updated_by" type="integer"></property>
  <property name="status" column="status" type="string"></property>
  </class>
</hibernate-mapping>