<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.sales">
  <class name="Phone_Category" table="phone_category">
  <id name="category_id" column="category_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="category_name" column="category_name" type="string"></property>
  </class>
</hibernate-mapping>