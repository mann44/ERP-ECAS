<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean.purchase">
  <class name="Product_Category" table="product_category">
  <id name="pc_id" column="pc_id" type="integer">
  <generator class="native"></generator>
  </id>
  <property name="product_type_id" column="product_type_id" type="integer"></property>
  <property name="product_caegory" column="product_caegory" type="string"></property>
  <property name="description" column="description" type="string"></property>
  <property name="created_by" column="created_by" type="integer"></property>
  </class>
</hibernate-mapping>